<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="Translation">
        <property name="idGlossaryTranslation" type="int" />
        <property name="fkGlossaryKey" type="int" />
        <property name="fkLocale" type="int" />
        <property name="value" type="string" />
        <property name="isActive" type="bool" />
    </transfer>

    <transfer name="GlosssaryKey">
        <property name="idGlossaryKey" type="int"></property>
        <property name="key" type="string"></property>
        <property name="isActive" type="bool"></property>
    </transfer>

    <transfer name="TranslationCollection">
        <property name="TranslationKeys" type="GlossaryKey[]" singular="TranslationKey"></property>
        <property name="TranslationValues" type="Translation[]" singular="TranslationValue"></property>
    </transfer>

</transfers>

