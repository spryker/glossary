<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="GlossaryTranslation">
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <property name="idGlossaryTranslation" type="int" default="null" />
        <property name="fkGlossaryKey" type="int" default="null" />
        <property name="fkLocale" type="int" default="null" />
        <property name="value" type="string" default="null" />
        <property name="isActive" type="bool" default="true" />
    </transfer>

</transfers>

